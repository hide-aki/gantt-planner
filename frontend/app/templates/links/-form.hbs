<form {{action "save" on="submit"}}>
    <div>
        <label>
            Link Id
            {{input type="text" value=model.link_id}}
        </label>
    </div>
  <div>
    <label>
      Source Id
      {{input type="text" value=model.source_id}}
    </label>
  </div>
  <div>
    <label>
      Target Id
      {{input type="text" value=model.target_id}}
    </label>
  </div>
  <div>
    <label>
      Type
      {{input type="text" value=model.code}}
    </label>
  </div>
  <div>
    <input type="submit" value="Save">
  </div>
</form>
